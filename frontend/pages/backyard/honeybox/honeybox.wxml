<template name="honeybox">
    <image bindtap="on_click_honey" id="{{item.start_time}}" src="../../image/garden_fm@2x.png" style="position: absolute; top: calc(20rpx + {{item.x}} * 86rpx); left: calc(20rpx + {{item.y}} * 187rpx); width: 57rpx; height: 67rpx;z-index: 5;"></image>
    <text bindtap="on_click_honey" id="{{item.start_time}}" style="position: absolute; top: calc(50rpx + {{item.x}} * 86rpx); left: calc(77rpx + {{item.y}} * 187rpx); width: 87rpx; height: 30rpx;background-color:#E6B53C; text-align: center; font-family: PingFangSC-Semibold; font-size: 22rpx; color: #472E00; border: 2rpx solid #472E00; border-radius: 6rpx;z-index: 5;">{{item.honey}}ml</text>
</template>
